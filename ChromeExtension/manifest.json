{
  "manifest_version": 2,

  "name": "GameOn",
  "description": "Shows shoppers the complexity of each board game on the page they are browsing.",
  "version": "1.0.1",
  "author": "Pamela Moriarty",


  "permissions": [
    "activeTab",
    "https://gameonapp.xyz/"
  ],
 
    "browser_action":{
      "default_icon": "icon.jpg"
    },
  "content_scripts": [{
      "js": ["content.js"],
      "matches": ["https://www.target.com/c/board-games-puzzles-toys/*"]
  }],
  "background":{
    "scripts":["background.js"],
    "persistent": false
}

}

